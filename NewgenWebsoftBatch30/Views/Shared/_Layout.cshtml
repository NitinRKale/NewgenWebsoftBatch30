<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Project Batch</title>

    <!-- Mobile Specific Metas
    ================================================== -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Construction Html5 Template">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">

    <!-- Favicon
    ================================================== -->
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@400;700&display=swap" rel="stylesheet">
    <link href="~/pharmaasset/fonts/icomoon/style.css" rel="stylesheet" />


    <!-- Pharma Template css -->

    <link rel="stylesheet" href="~/pharmaasset/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/pharmaasset/fonts/flaticon/font/flaticon.css">
    <link rel="stylesheet" href="~/pharmaasset/css/magnific-popup.css">
    <link rel="stylesheet" href="~/pharmaasset/css/jquery-ui.css">
    <link rel="stylesheet" href="~/pharmaasset/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/pharmaasset/css/owl.theme.default.min.css">
    @* <link rel="stylesheet" href="~/pharmaasset/css/aos.css"> *@
    <link rel="stylesheet" href="~/pharmaasset/css/style.css">
    <link href="~/css/account.css" rel="stylesheet" />

    <!-- SweetAlert2 CSS -->
    <link href="~/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <link href="~/datatables/datatables.min.css" rel="stylesheet" />


    <!-- Javascript Files
    ================================================== -->
    <script src="~/pharmaasset/js/jquery-3.3.1.min.js"></script>
    <script src="~/pharmaasset/js/jquery-ui.js"></script>
    <script src="~/pharmaasset/js/popper.min.js"></script>
    <script src="~/pharmaasset/js/bootstrap.min.js"></script>
    <script src="~/pharmaasset/js/owl.carousel.min.js"></script>
    <script src="~/pharmaasset/js/jquery.magnific-popup.min.js"></script>
    @* <script src="~/pharmaasset/js/aos.js"></script> *@
    <script src="~/pharmaasset/js/main.js"></script>

    <!-- DataTables JS (correct file name) -->
    <script src="~/datatables/datatables.min.js"></script>

    <!-- Bootstrap JS -->
    @*  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script> *@

    <!-- SweetAlert2 JS (only one needed) -->
    <script src="~/sweetalert2/sweetalert2.min.js"></script>


</head>
<body>

    <!-- site wrap start -->
    <div class="site-wrap">

        <!-- Navbar start -->
        <div class="site-navbar py-2">

            <div class="search-wrap">
                <div class="container">
                    <a href="#" class="search-close js-search-close"><span class="icon-close2"></span></a>
                    <form action="#" method="post">
                        <input type="text" class="form-control" placeholder="Search keyword and hit enter...">
                    </form>
                </div>
            </div>

            <div class="container">
                <div class="d-flex align-items-center justify-content-between">
                    <div class="logo">
                        <div class="site-logo">
                            <a href="index.html" class="js-logo-clone"><strong class="text-primary">Newgen</strong></a>
                        </div>
                    </div>
                    <div class="main-nav d-none d-lg-block">
                        <nav class="site-navigation text-right text-md-center" role="navigation">
                            <ul class="site-menu js-clone-nav d-none d-lg-block">                                
                                <li class="nav-item active">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Department" asp-action="Index">Department</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Employee" asp-action="Index">Employee</a>
                                </li>
                                <li class="nav-item has-children">
                                    <a href="#">Department</a>
                                    <ul class="dropdown">
                                        <li class="nav-item text-dark"><a href="#">Supplements</a></li>
                                        <li class="nav-item text-dark has-children">
                                            <a class="nav-link text-dark" href="#">Vitamins</a>
                                            <ul class="dropdown">
                                                <li><a class="nav-link text-dark" href="#">Supplements</a></li>
                                                <li><a class="nav-link text-dark" href="#">Vitamins</a></li>
                                                <li><a class="nav-link text-dark" href="#">Diet &amp; Nutrition</a></li>
                                                <li><a class="nav-link text-dark" href="#">Tea &amp; Coffee</a></li>
                                            </ul>
                                        </li>
                                        <li class="nav-item"><a href="#">Diet &amp; Nutrition</a></li>
                                        <li class="nav-item"><a href="#">Tea &amp; Coffee</a></li>

                                    </ul>
                                </li>
                                <li class="nav-item"><a class="nav-link text-dark" href="about.html">About</a></li>
                                <li class="nav-item"><a class="nav-link text-dark" href="contact.html">Contact</a></li>

                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Account" asp-action="Logout">Logout</a>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <div class="icons">
                        <a href="#" class="icons-btn d-inline-block js-search-open"><span class="icon-search"></span></a>
                        <a href="cart.html" class="icons-btn d-inline-block bag">
                            <span class="icon-shopping-bag"></span>
                            <span class="number">2</span>
                        </a>
                        <a href="#" class="site-menu-toggle js-menu-toggle ml-3 d-inline-block d-lg-none">
                            <span class="icon-menu"></span>
                        </a>
                    </div>
                </div>
            </div>
        </div>

        <!-- Navbar End -->
       
        <!-- Render Body start -->
        <br/>
        <br />
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                    </>
                </div>
            </div>
        </div>
        <!-- Render Body End -->
      
        <!-- footer start -->
        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2025 - Newgen WebSoft - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>
        <!-- footer end -->
        @await RenderSectionAsync("Scripts", required: false)

    </div>
    <!-- site-wrap end -->
</body>
</html>
